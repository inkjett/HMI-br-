Column_Hist[5] = Main_tables_1.Table_History_mode.Colum_6;
Column_Hist[6] = Main_tables_1.Table_History_mode.Colum_7;
Column_Hist[7] = Main_tables_1.Table_History_mode.Colum_8;
Column_Hist[8] = Main_tables_1.Table_History_mode.Colum_9;
Column_Hist[9] = Main_tables_1.Table_History_mode.Colum_10;
Column_Hist[10] = Main_tables_1.Table_History_mode.Colum_19;
Column_Hist[11] = Main_tables_1.Table_History_mode.Colum_20;

columnCount = Math.min(Column_Oper.length, Column_Hist.length); //Ищем минимальное количество столбцов, параметры которых можно передать

//Импорт параметров между таблицами
if (ActiveMode == 0)
{
	//Сброс сортировки
	for (let i = 0; i < columnCount; i++)
	{
		Column_Oper[i].SortingEnabled = false;
	}
	Main_tables.Table_Operative_mode.ApplySort();
	
	for (let i = 0; i < Column_Hist.length; i++)
	{
		Column_Oper[i].Title = Column_Hist[i].Title;
		Column_Oper[i].Hidden = Column_Hist[i].Hidden;
		Column_Oper[i].WidthResizeMode = Column_Hist[i].WidthResizeMode;
		Column_Oper[i].Width = Column_Hist[i].Width;
		Column_Oper[i].SortingEnabled = Column_Hist[i].SortingEnabled;
		Column_Oper[i].SortOrder = Column_Hist[i].SortOrder;
		Column_Oper[i].SortPriority = Column_Hist[i].SortPriority;
	}
	Main_tables.Table_Operative_mode.ApplySort();
}
else
{
	//Сброс сортировки
	for (let i = 0; i < columnCount; i++)
	{
		Column_Hist[i].SortingEnabled = false;
	}
	Main_tables_1.Table_History_mode.ApplySort();
	
	for (let i = 0; i < columnCount; i++)
	{
		Column_Hist[i].Title = Column_Oper[i].Title;
		Column_Hist[i].Hidden = Column_Oper[i].Hidden;
		Column_Hist[i].WidthResizeMode = Column_Oper[i].WidthResizeMode;
