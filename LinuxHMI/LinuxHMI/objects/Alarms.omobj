					if (vb){ 		// если НЕ пора прерывать бесконечный цикл
							Queue[i].MoveFirst();				Range[i].vIndex=1;
							SetSound(i); return;
					}else{
						Range[i].bSkipped=true;			// Устанавливаем признак, что бесконечный поток прерван
					} 
					
				}else{	// если отыграно заданное количество циклов
					Range[i].bSkipped=true;			// Устанавливаем признак, что счетчик отработан и повторного вычета не будет
					Range[i].PriorityCount = Range[i].SoundPriority;
					if (Range[i].nSoundRepeats!=0){
						Queue[i].Clear(); Range[i].vIndex= 0;
						Range[i].QueueLength=0;
					}else{
						Range[i].SoundCount = 1; // ставим на 1 больше так как 1 отнимится при переходе на начало очереди
					}
					i=0; // начинаем цикл с начала
				}
			}
		}//*/
	}
