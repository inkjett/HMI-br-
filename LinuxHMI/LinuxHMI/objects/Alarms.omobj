	<do access-modifier="private" name="SkipCurrentSound" display-name="SkipCurrentSound" ver="2" type="" type-id="">
		<body kind="javascript">
			<![CDATA[//Фукция формирует очередь звуков событий и воспроизводит их
if (Mute.Value) return; //Выход, если режим без звука
if (Main_panel.private.SoundQueue2!=""){ // Формирование очереди звуков в виде строки.
		let sounds = Main_panel.private.SoundQueue2.split(","); //Преобразуем очередь звуков в элементы массива
		Sound_1.Path = sounds[0]+".wav"; Sound_1.Active = true; //Воспроизводим первый элемент
		sounds.shift();//Удаляем первый элемент
		Main_panel.private.SoundQueue2 = sounds.toString();//Обновляем очередь воспроизведения
		log(Main_panel.private.SoundQueue2);
		return;
}
log(Main_panel.private.SoundQueue2);
var Queue=[],Range=[];
Queue[1]= Main_tables.Table_Operative_mode.ApAlarmQueue_1;
Queue[2]= Main_tables.Table_Operative_mode.ApAlarmQueue_2;
Queue[3]= Main_tables.Table_Operative_mode.ApAlarmQueue_3;
Queue[4]= Main_tables.Table_Operative_mode.ApAlarmQueue_4;
Range[1]= Main_panel.private.Range1;
Range[2]= Main_panel.private.Range2;
Range[3]= Main_panel.private.Range3;
Range[4]= Main_panel.private.Range4;

function SetSound(ii){//Функция считывает набор звуков сообещния, формирует очередь, воспроизводит
			var sAlarm, sSounds, tempArray;
			sAlarm = Queue[ii].CurrentAlarm(); //Получаем текущий аларм
			sAlarm = JSON.parse(sAlarm); 
			sSounds = sAlarm.sound; //Заполняем массив звуков данными из столбца sound аларма
			sSounds = sSounds.split(";");//Отделяем звук объекта от звука сообщения
			if (/,/g.test(sSounds[1])) //Проверяем, есть ли несколько звуков у сообщения
			{
				tempArray = sSounds[1].split(","); //Разделяем набор звуков поэлементно, формируем массив
				sSounds.pop();//Удаляем набор звуков из результирующего массива
				for (let i = 0; i < tempArray.length; i++) //Добавляем в массив по отдельности каждый указанный звук
				{
					if (tempArray[i] != "") sSounds.push(tempArray[i]);
				}
			}
			if (sSounds[0] == "") sSounds.shift(); //Если нет звука объекта, удаляем первый элемент
			for (let i = 0; i < sSounds.length; i++) //Идем проверка на запись расширения в названии звукового файла - удаляем
			{
				
				if (/(.wav)/g.test(sSounds[i])) sSounds[i] = sSounds[i].replace(/(.wav)/g,"");
				sSounds[i] = sSounds[i].trim();//Удаляем лишние пробелы в конце и в начале
				log("sSounds["+i+"]: " + sSounds[i]);
			}
			if (sSounds[0] != "") Sound_1.Path = sSounds[0]+".wav"; Sound_1.Active = true; //Ставим на воспроизведение первый звук в очереди
			if (sSounds.length > 1) // Если событие содержит несколько звуков, формируем очередь
			{
				sSounds.shift();//Удаляем проигранный элемент
				Main_panel.private.SoundQueue2 = sSounds.toString(); //Формируем очередь в виде строки
			}	
			Range[ii].bSkipped = false;
	var st="",j
	for(j=1;j<=4;j++){
		if (j==i) st =st +Range[j].vIndex +"<" +Queue[j].Length +"_" +Range[j].PriorityCount +"    " ;
		else		  st =st +Range[j].vIndex +"/" +Queue[j].Length +"_" +Range[j].PriorityCount +"    ";
	}
//	log(st);
}//*/


var i=1, j,  bInterrupt, vnHiPriority=0, vst;
//============================================================================================================									

while (i<=4)					// ЦИКЛ опроса готовности очередей
{
	if (Queue[i].Active && Range[i].SoundEnable) // Если очередь  активна (не пуста)
	{
		if (Range[i].nSoundRepeats==0 && vnHiPriority==0) 
			vnHiPriority = i;							// Запоминаем номер очереди с бесконечным проигрыванием
							//-- ПОДГОТОВКА СЧЕТЧИКА СООБЩЕНИЙ ПРИ ИЗМЕНЕНИИ СОСТАВА ОЧЕРЕДИ  --
	if (Range[i].QueueLength!=Queue[i].Length){ // Если состав очереди изменился
			Range[i].QueueLength=Queue[i].Length;	
			if (Range[i].nSoundRepeats!=0){
				Range[i].SoundCount = Range[i].nSoundRepeats;
			}else{													// Если в очереди задано бесконечное проигрывание звуков
				if (vnHiPriority==i) {							// Ранее таких очередей не зафиксировано
					if (Range[i].SoundCount<100){		// Если задано не бесконечное количество повторов
						Range[i].SoundCount = 0x7fff; // Максимальное значение (int2) для бесконечного цикла				
					}
				} else {		// если есть очереди с бесконечным проигрыванием более высокого приоритета
					Range[i].SoundCount = 1;				// устанавливаем количество проигрываний = 1
				}
				for (j=i+1;j<=4;j++){				// Для остальных очередей заполняем счетчики приоритетов
					Range[j].PriorityCount = Range[j].SoundPriority;	// значением из настроек
				}
			}
		}//*/
							
