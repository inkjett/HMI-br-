			<object access-modifier="private" name="AckContinue" display-name="AckContinue" uuid="9d27da94-d903-406d-a57d-3d48d7b61716" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="2">
				<designed target="Enabled" value="true" ver="2"/>
				<designed target="InvokeTrigger" value="false" ver="2"/>
				<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="3" event="Invoked">
					<body kind="om">
						<![CDATA[if (nAckMode==1)		// РЕЖИМ КВИТИРОВАНИЯ ВЫДЕЛЕННЫХ СООБЩЕНИЙ
{
sSelect:string = Main_tables.Table_Operative_mode.GetSelectedRows();  // Получаем список выделенных строк ввиде "[3,4,5,6]"
//DebugTool_1.Log(sSelect);
if (Str.Length(sSelect)>2) sSelect = Str.SubString(sSelect,1,Str.Length(sSelect)-2); // Освобождаемся от квадратных скобок
else return;
n:int4;
sSelectI:string;

while (Str.Length(sSelect)>0)
{
	n= Str.IndexOf(sSelect,",");
	if (n>0){
		sSelectI= Str.SubString(sSelect,0,n);
		sSelect = Str.SubString(sSelect,n+1);
	}else{
		sSelectI= sSelect;
		sSelect= "";
	}
	nSelect:uint4 = Str.ToUint4(sSelectI,99999);
//	DebugTool_1.Log(sSelectI);
	if (nSelect==99999) return ;
//	if (Main_tables.Table_Operative_mode.RowsOrder==1) // Если добавление строк сверху, инвертируем номер выбранной строки
//		nSelect= TypeConvert.ToUint4( Main_tables.Table_Operative_mode.GetRowsCount()-nSelect)-1;
	AckSost:var = Main_tables.Table_Operative_mode.GetCellData(nSelect,6); //Состояние "Квитировано" в случае, если квитирование не требуется равно EMPTY, если квитировано = true, если неквитировано = false;
	Acked:bool = true;
	if (!Variant.IsEmpty(AckSost)) Acked = Variant.ToBool(AckSost,false); //Если поле "Квитировано" не пустое (требуется квитирование) то смотрим на статус квитирования
	if (!Acked) 
	{
		Src:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(nSelect,4),"");
		ConditionName:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(nSelect,10),"");
		ActiveTime:uint8 = Variant.ToUint8(Main_tables.Table_Operative_mode.GetCellData(nSelect,1),0);
		Cookie:uint4 = Variant.ToUint4(Main_tables.Table_Operative_mode.GetCellData(nSelect,14),0);
		//DebugTool_1.Log(Str.ToString(ActiveTime)+","+Str.ToString(Cookie));
		ApSource.Ack(Src,ConditionName,Main_panel.private.AckComment,Main_panel.private.UserName,ActiveTime,Cookie);
	}
}
}
if (nAckMode==2)			// РЕЖИМ КВИТИРОВАНИЯ ВСЕХ СООБЩЕНИЙ
{
	n:uint4;
	Main_tables.Table_Operative_mode.TableIterator_1.FirstSelected();
	while (!Main_tables.Table_Operative_mode.TableIterator_1.End()){
		n= Main_tables.Table_Operative_mode.TableIterator_1.CurrentRow;
		AckSost:var = Main_tables.Table_Operative_mode.GetCellData(n,6); //Состояние "Квитировано" в случае, если квитирование не требуется равно EMPTY, если квитировано = true, если неквитировано = false;
		Acked:bool = true;
		if (!Variant.IsEmpty(AckSost)) Acked = Variant.ToBool(AckSost,false); //Если поле "Квитировано" не пустое (требуется квитирование) то смотрим на статус квитирования
		if (!Acked)
		{
			Src:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(n,4),"");
			ConditionName:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(n,10),"");
			ActiveTime:uint8 = Variant.ToUint8(Main_tables.Table_Operative_mode.GetCellData(n,1),0);
			Cookie:uint4 = Variant.ToUint4(Main_tables.Table_Operative_mode.GetCellData(n,14),0);
			//DebugTool_1.Log(Str.ToString(ActiveTime)+","+Str.ToString(Cookie));

			ApSource.Ack(Src,ConditionName,Main_panel.private.AckComment,Main_panel.private.UserName,ActiveTime,Cookie);
		}
		Main_tables.Table_Operative_mode.TableIterator_1.Next();
	}
//	dt:uint8 =  DateTime.Now() - StartTime;
//	Text_1.Text = Str.ToString(DateTime.Second(dt))+":"+Str.ToString(DateTime.MSecond(dt)); // вычисление затрат времени на квитирование
}
if (nAckMode==3)		// РЕЖИМ КВИТИРОВАНИЯ ПОСЛЕДОВАТЕЛЬНОСТИ (ВСЕХ СООБЩЕНИЙ ИСТОЧНИКА)
{
n:int4;
sSelectI:string;
sSelect:string = GetAlarmsSequence();  // Получаем последовательность сообщений источника в виде "3,4,5,6"
DebugTool_1.Log(sSelect);

while (Str.Length(sSelect)>0)
{
	n= Str.IndexOf(sSelect,",");
	if (n>0){
		sSelectI= Str.SubString(sSelect,0,n);
		sSelect = Str.SubString(sSelect,n+1);
	}else{
		sSelectI= sSelect;
		sSelect= "";
	}
	nSelect:uint4 = Str.ToUint4(sSelectI,99999);
//	DebugTool_1.Log(sSelectI);
	if (nSelect==99999) return ;
//	if (Main_tables.Table_Operative_mode.RowsOrder==1) // Если добавление строк сверху, инвертируем номер выбранной строки
//		nSelect= TypeConvert.ToUint4( Main_tables.Table_Operative_mode.GetRowsCount()-nSelect)-1;
	AckSost:var = Main_tables.Table_Operative_mode.GetCellData(nSelect,6); //Состояние "Квитировано" в случае, если квитирование не требуется равно EMPTY, если квитировано = true, если неквитировано = false;
	Acked:bool = true;
	if (!Variant.IsEmpty(AckSost)) Acked = Variant.ToBool(AckSost,false); //Если поле "Квитировано" не пустое (требуется квитирование) то смотрим на статус квитирования
	if (!Acked) 
	{
		Src:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(nSelect,4),"");
		ConditionName:string = Variant.ToString( Main_tables.Table_Operative_mode.GetCellData(nSelect,10),"");
		ActiveTime:uint8 = Variant.ToUint8(Main_tables.Table_Operative_mode.GetCellData(nSelect,1),0);
		Cookie:uint4 = Variant.ToUint4(Main_tables.Table_Operative_mode.GetCellData(nSelect,14),0);
		//DebugTool_1.Log(Str.ToString(ActiveTime)+","+Str.ToString(Cookie));

		ApSource.Ack(Src,ConditionName,Main_panel.private.AckComment,Main_panel.private.UserName,ActiveTime,Cookie);
	}
}
}
nAckMode=0;]]>
					</body>
				</do-on>
			</object>
