			<do access-modifier="private" name="LoadHistory" display-name="LoadHistory" ver="3" type="" type-id="">
				<body kind="om">
					<![CDATA[//Загружаем историю
dateStart: string = Main_panel.ToolBar.RecHistory.DayMonthYear_start.Text;
dateFinal: string = Main_panel.ToolBar.RecHistory.DayMonthYear_final.Text;
timeStart: string = Main_panel.ToolBar.RecHistory.Time_start.Text;
timeFinal: string = Main_panel.ToolBar.RecHistory.Time_final.Text;

if(ActiveMode==1)
{
	Main_panel.ToolBar.RecHistory.DayMonthYear_start.FontColor = 0xff000000;
	Main_panel.ToolBar.RecHistory.DayMonthYear_final.FontColor = 0xff000000;
	Main_panel.ToolBar.RecHistory.Time_start.FontColor = 0xff000000;
	Main_panel.ToolBar.RecHistory.Time_final.FontColor = 0xff000000;
	Main_tables_1.Table_History_mode.ClearData();
	
	if (DateTime.Parse(dateStart) == 0) dateStart = String.Concat("0",dateStart);
	if (DateTime.Parse(dateFinal) == 0) dateFinal = String.Concat("0",dateFinal);
	//Если даты некорректны, то меняем цвет шрифта на полях дат, если нет - запрашиваем данные с сервера
	if (!SetHistoricalInterval(DateTime.Parse(dateStart + " " + timeStart)-Main_panel.private.TimeZone(),DateTime.Parse(dateFinal + " " + timeFinal)-Main_panel.private.TimeZone()))
	{
		Main_panel.ToolBar.RecHistory.DayMonthYear_start.FontColor = 0xffff0000;
		Main_panel.ToolBar.RecHistory.DayMonthYear_final.FontColor = 0xffff0000;
		Main_panel.ToolBar.RecHistory.Time_start.FontColor = 0xffff0000;
		Main_panel.ToolBar.RecHistory.Time_final.FontColor = 0xffff0000;
	}
	StatePanel.RequestFilterStateChange();//Обновляем состояние фильтра в строке
}
]]>
				</body>
