				Main_panel.ToolBar.btnSndMute.HideFlag.Value = ToolBoxSet[0][14];
				Main_panel.ToolBar.btnSort.HideFlag.Value = ToolBoxSet[0][15];
				Main_panel.ToolBar.btnLogin.HideFlag.Value = ToolBoxSet[0][16];
				break;
			//Чтение конфигурации важностей
			case 2:
				SeveritySet = JSON.parse(ConfigJSONArray[i]);
				Main_tables.Table_Operative_mode.ApAlarmQueue_1.JsonFilter = SeveritySet[0][1];
				Main_tables.Table_Operative_mode.ApAlarmQueue_2.JsonFilter = SeveritySet[0][2];
				Main_tables.Table_Operative_mode.ApAlarmQueue_3.JsonFilter = SeveritySet[0][3];
				Main_tables.Table_Operative_mode.ApAlarmQueue_4.JsonFilter = SeveritySet[0][4];

				Main_panel.private.Range1.SoundEnable = SeveritySet[1][1];
				Main_panel.private.Range1.nSoundRepeats = SeveritySet[1][2];
				Main_panel.private.Range1.SoundPriority = SeveritySet[1][3];
				Main_panel.private.Range2.SoundEnable = SeveritySet[2][1];
				Main_panel.private.Range2.nSoundRepeats = SeveritySet[2][2];
				Main_panel.private.Range2.SoundPriority = SeveritySet[2][3];
				Main_panel.private.Range3.SoundEnable = SeveritySet[3][1];
				Main_panel.private.Range3.nSoundRepeats = SeveritySet[3][2];
				Main_panel.private.Range3.SoundPriority = SeveritySet[3][3];
				break;
			//Чтение конфигурации сортировки
			case 3:
				SortingSet = JSON.parse(ConfigJSONArray[i]);
				//Сбрасываем текущую сортировку
				for (let i = 0; i < Column.length; i++)
				{
					Column[i].SortingEnabled = false;
				}
				Main_tables.Table_Operative_mode.ApplySort();
				Main_tables.Table_Operative_mode.ApplySort();
				//Применение загруженной конфигурации
				for (let i = 0; i < Column.length; i++)
				{
					Column[i].SortingEnabled = SortingSet[i][1];
					Column[i].SortPriority = SortingSet[i][2];
					Column[i].SortOrder = SortingSet[i][3];
				}
				Main_tables.Table_Operative_mode.ApplySort();
				Main_tables.Table_Operative_mode.ApplySort();
		}
	}
	catch(error)
	{
		continue;
	}
}
return true;
]]>
