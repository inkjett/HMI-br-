			<do access-modifier="private" name="GetAlarmsSequence" display-name="GetAlarmsSequence" ver="3" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
				<body kind="javascript">
					<![CDATA[//Функция поиска последовательности сообщений источника. Возвращает строку с номерами строк таблицы
var selRows = ""; //Временная строка
var sourceArray = []; //Массив источников
var currentSource = ""; //Имя источника текущей строки
var currentAck; //Статус квитировано текущей строки

selRows = JSON.parse(Main_tables.Table_Operative_mode.GetSelectedRows());
selRows = selRows.toString();
sourceArray = selRows.split(",");
selRows = "";

//Заполняем массив источниками неквитированных сообщений в соответствии с выделенными номерами строк
for (let i = 0; i < sourceArray.length; i++)
{
	currentSource = Main_tables.Table_Operative_mode.GetCellData(sourceArray[i],4);
	currentAck = Main_tables.Table_Operative_mode.GetCellData(sourceArray[i],6);
	if (!currentAck && currentAck != null && (sourceArray.indexOf(currentSource) == -1)) sourceArray[i] = currentSource;
	else //Удаляем элемент массива, если сообщение квитировано, не требует квитирования или такой источник уже есть в массиве
	{
		sourceArray.splice(i,1);
		i -= 1;
	}
}

//Ищем в таблице сообщения с полученнымими источниками и записываем их номера в строку
Main_tables.Table_Operative_mode.TableIterator_1.FirstSelected();
while (!Main_tables.Table_Operative_mode.TableIterator_1.End())
{
	currentSource = Main_tables.Table_Operative_mode.GetCellData(Main_tables.Table_Operative_mode.TableIterator_1.CurrentRow,4);
	currentAck = Main_tables.Table_Operative_mode.GetCellData(Main_tables.Table_Operative_mode.TableIterator_1.CurrentRow,6);
	for (let i = 0; i < sourceArray.length; i++)
	{
		if (currentSource == sourceArray[i] && !currentAck && currentAck != null) selRows += Main_tables.Table_Operative_mode.TableIterator_1.CurrentRow + ",";
	}
	Main_tables.Table_Operative_mode.TableIterator_1.Next();
}
if (selRows.length > 0) selRows = selRows.substring(0,selRows.length-1);
return selRows;]]>
				</body>
			</do>
