		Column_Hist[i].SortOrder = Column_Oper[i].SortOrder;
		Column_Hist[i].SortPriority = Column_Oper[i].SortPriority;
	}
	Main_tables_1.Table_History_mode.ApplySort();
}

]]>
				</body>
