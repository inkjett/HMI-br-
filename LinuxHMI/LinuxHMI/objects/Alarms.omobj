						bInterrupt=true;
					}
				}
//					log(vst);
			}else{
				if (Range[i].PriorityCount>0) bInterrupt=true;
			}
		}//*/
							//-- ПРОИГРЫВАНИЕ ТЕКУЩИХ ЗВУКОВ И ОБРАБОТКА СЧЕТЧИКОВ --
		if (Range[i].SoundCount!=0 && !bInterrupt)
		{
			if (Queue[i].MoveNext()){
				Range[i].vIndex++;
				SetSound(i);	// Запускаем звук из текущего элемента очереди
				return;
			}else{				// если достигнут конец очереди
				if (!Range[i].bSkipped) Range[i].SoundCount --;
				if (Range[i].SoundCount>0){
					var vb=true;								// 
					if (i==vnHiPriority) 					// если закончился очередной цикл бесконечного проигрыания
						for (j=i+1;j<=4;j++){				// для остальных потоков
							if (!Range[i].bSkipped) Range[j].PriorityCount --;	// уменьшаем счетчики приоритета
