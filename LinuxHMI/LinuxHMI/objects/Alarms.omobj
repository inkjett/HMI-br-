			<do access-modifier="private" name="func_SettingsToFile" display-name="func_SettingsToFile" ver="2" type="" type-id="">
				<body kind="javascript">
					<![CDATA[//Функция сохранения конфигураций в файл
var FolderPath;
if (ConfigurationFolder == "") ConfigurationFolder = ConfigPathDefault;//Определение пути записи файла
//Проверяем, является ли указанный путь абсолютным
if (/^(.+):(\\.*)$/.test(ConfigurationFolder))FolderPath = ConfigurationFolder;
else FolderPath = File.ProjectFolder + ConfigurationFolder;
var ConfigTable="tableview.json";//вид таблицы
var ConfigToolbox="toolbox.json";//видимость элементов панели инструментов
var ConfigSeverity="severitygroups.json";//распределение групп важностей и проигрывания звуков
var ConfigSorting="sorting.json";//сортировка

var ConfigTablePath = File.CombinePath( FolderPath, ConfigTable);
var ConfigToolboxPath = File.CombinePath( FolderPath, ConfigToolbox);
var ConfigSeverityPath = File.CombinePath( FolderPath, ConfigSeverity);
var ConfigSortingPath = File.CombinePath( FolderPath, ConfigSorting);

//Создание структуры сохраняемых параметров
var TableView = 
[ 
["Colum_1"],
["Colum_2"],
["Colum_3"],
["Colum_4"],
["Colum_5"],
["Colum_6"],
["Colum_7"],
["Colum_8"],
["Colum_9"],
["Colum_10"],
["Colum_19"],
["Colum_20"],
["RowsHeight"]
]
var ToolBoxSet =
[
["HideButton"]
]
var SeveritySet = 
[
["Severity"],
["SoundRange1"],
["SoundRange2"],
["SoundRange3"]
]
var SortingSet = 
[ 
["Colum_1"],
["Colum_2"],
["Colum_3"],
["Colum_4"],
["Colum_5"],
["Colum_6"],
["Colum_7"],
["Colum_8"],
["Colum_9"],
["Colum_10"],
["Colum_19"],
["Colum_20"],
]
// Проверка на существование папки конфигурации
if (!File.Exists(FolderPath)) File.CreateRecursiveFolder(FolderPath);

//Запись параметров активной таблицы
if (ActiveMode.Value == 0)
{
	//Запись настроек ширины столбцов во временные переменные в случае, если они видны
	if (Main_tables.Table_Operative_mode.Colum_1.Width != 0) Main_panel.private.widthColumn.Column_1 = Main_tables.Table_Operative_mode.Colum_1.Width;
	if (Main_tables.Table_Operative_mode.Colum_2.Width != 0) Main_panel.private.widthColumn.Column_2 = Main_tables.Table_Operative_mode.Colum_2.Width;
	if (Main_tables.Table_Operative_mode.Colum_3.Width != 0) Main_panel.private.widthColumn.Column_3 = Main_tables.Table_Operative_mode.Colum_3.Width;
	if (Main_tables.Table_Operative_mode.Colum_4.Width != 0) Main_panel.private.widthColumn.Column_4 = Main_tables.Table_Operative_mode.Colum_4.Width;
	if (Main_tables.Table_Operative_mode.Colum_5.Width != 0) Main_panel.private.widthColumn.Column_5 = Main_tables.Table_Operative_mode.Colum_5.Width;
	if (Main_tables.Table_Operative_mode.Colum_6.Width != 0) Main_panel.private.widthColumn.Column_6 = Main_tables.Table_Operative_mode.Colum_6.Width;
	if (Main_tables.Table_Operative_mode.Colum_7.Width != 0) Main_panel.private.widthColumn.Column_7 = Main_tables.Table_Operative_mode.Colum_7.Width;
	if (Main_tables.Table_Operative_mode.Colum_8.Width != 0) Main_panel.private.widthColumn.Column_8 = Main_tables.Table_Operative_mode.Colum_8.Width;
	if (Main_tables.Table_Operative_mode.Colum_9.Width != 0) Main_panel.private.widthColumn.Column_9 = Main_tables.Table_Operative_mode.Colum_9.Width;
	if (Main_tables.Table_Operative_mode.Colum_10.Width != 0) Main_panel.private.widthColumn.Column_10 = Main_tables.Table_Operative_mode.Colum_10.Width;
	if (Main_tables.Table_Operative_mode.Colum_19.Width != 0) Main_panel.private.widthColumn.Column_19 = Main_tables.Table_Operative_mode.Colum_19.Width;
