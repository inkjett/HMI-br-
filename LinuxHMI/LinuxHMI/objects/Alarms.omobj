		bInterrupt = false;									// флаг прерывания бесконечного проигрывания звуков
		if (Range[i].nSoundRepeats==0)				// Если это очередь с бесконечным проигрыванием
		{
			if (i==vnHiPriority){								// Это основная очередь ?
