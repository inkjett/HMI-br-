				j=0; vb=true;
				while (j<n && vb){
					if(EUs[j]==Main_Panel.private.NowParams.Units[i]) vb=false;
					j= j+1; 
				}
				if (vb){
					EUs[n]= Main_Panel.private.NowParams.Units[i];
					n=n+1;
				}
			}
		}
	}
	if (n>0){
		vEU= ","+ EUs[0];
		i=1;
		while (i<n){
			vEU= vEU +"," +EUs[i];
			i=i+1;
		}
	}
	WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[0].Unit =  vEU;
	WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Unit = vEU;
}
else
{
	for (i=0; i<WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y.Length ;i++){
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].Unit = "";
