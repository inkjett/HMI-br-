		if (Main_Panel.private.GetItemPath(i)==tag) n0=i;
		i=i+1;
	}
	if (nAxis==0||n0==0){
		DebugTool_1.Log("SetGraphicAxis. Индекс для одного из параметров не определен!");
		return;
	}	

	id:int4 = WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[nAxis].GetID();  // получаем идентификатор шкалы
	WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[nAxis].Visible = true;
	if (n0==1) WS.RS.Main_Charts.Chart_Operative_mode.Line_1.SetAxisById(id); // делаем привязку к новой шкале
	if (n0==2) WS.RS.Main_Charts.Chart_Operative_mode.Line_2.SetAxisById(id);
