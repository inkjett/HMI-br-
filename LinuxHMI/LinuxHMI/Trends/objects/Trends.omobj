						<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="3" event="ChangeValue">
							<body kind="javascript">
								<![CDATA[//return;
var Line=[];
Line[1]= WS.RS.Main_Charts.Chart_Operative_mode.Line_1;
Line[2]= WS.RS.Main_Charts.Chart_Operative_mode.Line_2;
Line[3]= WS.RS.Main_Charts.Chart_Operative_mode.Line_3;
Line[4]= WS.RS.Main_Charts.Chart_Operative_mode.Line_4;
Line[5]= WS.RS.Main_Charts.Chart_Operative_mode.Line_5;
Line[6]= WS.RS.Main_Charts.Chart_Operative_mode.Line_6;
Line[7]= WS.RS.Main_Charts.Chart_Operative_mode.Line_7;
Line[8]= WS.RS.Main_Charts.Chart_Operative_mode.Line_8;
Line[9]= WS.RS.Main_Charts.Chart_Operative_mode.Line_9;
Line[10]=WS.RS.Main_Charts.Chart_Operative_mode.Line_10;
var i=0;

var iStr=0;
while(iStr<WS.RS.Main_Legend.Table_1.GetRowsCount()){
	i= Main_Panel.private.NowParams.iItemInLegend[iStr];		// Получаем индекс графика, которому принадлежит строка легенды (индекс формируется в функции Main_Panel.private.ReFresh_LegendTable)
	if (i<Main_Panel.private.MAX_LINES){			// Если строка принадлежит оперативному режиму
		WS.RS.Main_Legend.Table_1.SetCellData(iStr, 3, Line[i].GetActiveControllineLabels());//данные
		var v1=Value+Main_Panel.private.TimeZone;
		WS.RS.Main_Legend.Table_1.SetCellData(iStr, 5, Main_Panel.private.VariantToDateTime(v1));//время
		WS.RS.Main_Legend.Table_1.SetCellData(iStr, 6, Item_1.Quality);							//качество
	}else{
		WS.RS.Main_Legend.Table_1.SetCellData(iStr, 3, "");//данные
		WS.RS.Main_Legend.Table_1.SetCellData(iStr, 5, "");//время
	}
	iStr= iStr +1;
}]]>
							</body>
						</do-on>
