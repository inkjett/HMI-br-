	if (n0==10) WS.RS.Main_Charts.Chart_History_mode.Line_10.SetAxisById(id);
	prevAxis:int4 = Main_Panel.private.NowParams.Item_numAxis[n0];
	Main_Panel.private.NowParams.Item_numAxis[n0]=nAxis;     // запоминем индекс новой шкалы для данного графика
	
	i=1;  //  проверяем в цикле нужна ли еще другим графикам старая шкала
	bNeed:bool = false;
	while (i<=10){
		if (Main_Panel.private.NowParams.Item_numAxis[i]==prevAxis) bNeed=true;
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_Y[i].AxisName= ""; // Очищаем все имена осей, чтобы сформировать заново
