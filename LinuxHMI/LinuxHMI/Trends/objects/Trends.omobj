							<body kind="om">
								<![CDATA[//DebugTool_1.Log("Клик Х");

if(Main_Panel.private.CursorMode==3)//добавление репера
{
	i:uint2=Main_Panel.private.NextReper();
	if (i>=WS.RS.Main_Charts.Chart_Operative_mode.ControlLine.Length) i=1;
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible=true;
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Value = clickValue;
	WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].setActive(true);
	WS.RS.Main_Charts.Chart_Operative_mode.private.lastAddReper=i;
}
else		// Выделение репера
{
	// отклонение точки клика от репера инициализируем максимальным числом - видимый диапазон 
	dx:uint8 = Variant.ToUint8(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max - WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Min,0);
	dt:uint8 = Variant.ToUint8(dx/50,0);	// максимальное отклонение точки клика от репера определяем +/- 2% от диапазона
	clickMin:uint8 = Variant.ToUint8(clickValue-dt,0);	// минимальная граница диапазона в который должен был попасть репер
	clickMax:uint8 = Variant.ToUint8(clickValue+dt,0);	// максимальная граница диапазона в который должен попасть репер
	i:uint4=1;		
	n:uint4=TypeConvert.ToUint4(WS.RS.Main_Charts.Chart_Operative_mode.ControlLine.Length); // количество реперов

	while(i<n){																													// перебираем реперы
		if (WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible){					// Если текущий репер видимый
			val:uint8 = Variant.ToUint8(WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Value,0);	// точка репера
			if ((val<clickMax) && (val>clickMin)){				// если точка репера попадает в границы диапазона +/-2% от точки клика
				vdx:uint8 = TypeConvert.ToUint8(Math.Abs(Variant.ToInt8(clickValue-val,0)));	// вычисляем отклонение репера от точки клика
				if (dx>vdx){ 																					// если отклонение репера меньше отклонения текущего репера
					WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].setActive(true);	// делаем текущий репер активным
					dx= vdx;																									// запоминаем отклонение выбранного репера
				}
			}
		}
		i=i+1;
	}
}//*/
]]>
