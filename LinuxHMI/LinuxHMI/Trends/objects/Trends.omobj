				<body kind="javascript">
					<![CDATA[//var fileName = FileManager.CombinePath(ExportTrendsPath,"00.json");

var Table = 
[
["LinesCount"],	// 0 - Количество линий графиков для сохранения
["TInterval"],		// 1 - Временной интервал
["GlobalRanges"],	// 2 - Признак индивидуального масштабирования
["IndexAxis"],		// 3 - Номер оси привязанной к графику
["AxisMin"],			// 4 - Минимум диапазона значений по оси Y
["AxisMax"],			// 5 - Максимум диапазона значений по оси Y
["AxisColor"],		// 6 - Цвет осей Y
["LineColor"],		// 7 - Цвет линий
["Description"],	// 8 - Описание сигнала
["Tag"],				// 9 - Адрес сигнала
["Units"],			// 10- Единицы измерения сигнала
["LinesWidth"],	// 11- Толщина линии
["Values"],			// 12- Значения
["Time"],				// 13- Время 
["Quality"],			// 14- Качество
["Minimum"],			// 15- Минимум
["Maximum"],			// 16- Мксимум
["Quantity"],		// 17- Количество
["AutoRange"],		// 18- Режим автоматического подбора диапазона значений
["RepersCount"],	// 19- Количество реперов
["RepersValues"],	// 20- Привязка реперов
["ActiveMode"],	// 21- Текущий режим (0-оперативный, 1-исторический)
["THistInterval"]	// 22- Временной интервал исторического режима
]

// Чтение из файла
if( FileManager.Exists( fileName ) )//проверка на существование файла
	{	
		var curContent = FileManager.ReadTextFile( fileName );
		Table = JSON.parse( curContent );
		//запись настроек в таблице оперативного режима	
		ExParams.LinesCount= Table[0][1];
		ExParams.Xmin= Table[1][1];
		ExParams.Xmax= Table[1][2];
		ExParams.bGlobalRanges = Table[2][1];
		ExParams.Ymin[0]= Table[4][1];
		ExParams.Ymax[0]= Table[5][1];
		ExParams.Ycolor[0]= Table[6][1];
//		log(Table[4][1]+" json "+Table[5][1]);
//		log(ExParams.Ymin[0]+" f8 "+ExParams.Ymax[0]);
		var i;
		for (i=1;i<=ExParams.LinesCount;i++){
			ExParams.Item_numAxis[i]= Table[3][i];
			ExParams.Ymin[i]= Table[4][i+1];
			ExParams.Ymax[i]= Table[5][i+1];
			ExParams.Ycolor[i]= Table[6][i+1];
			ExParams.LineColor[i]= Table[7][i];
			ExParams.Description[i]= Table[8][i];
			ExParams.ItemTag[i]= Table[9][i];
			ExParams.ItemEU[i]= Table[10][i];
			ExParams.LineWidth[i]= Table[11][i];
			ExParams.LegendValues[i]= Table[12][i];
			ExParams.LegendTime[i]= Table[13][i];
			ExParams.LegendQuality[i]= Table[14][i];
			ExParams.LegendMin[i]= Table[15][i];
			ExParams.LegendMax[i]= Table[16][i];
			ExParams.LegendQuantity[i]= Table[17][i];
		}
		ExParams.bAutoRange = Table[18][1];
		ExParams.RepersCount = Table[19][1];
		for (i=0;i<ExParams.RepersCount;i++){
			ExParams.RepersValues[i]= Table[20][i+1];
		}
		ExParams.ActiveMode = Table[21][1];
		ExParams.tHistRangeStart= Table[22][1];
		ExParams.tHistRangeEnd= Table[22][2];
		return true;
	}
	else 
	{
		return false;
	}]]>
				</body>
