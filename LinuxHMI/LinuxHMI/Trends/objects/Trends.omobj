				<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="2" event="ValueChanged">
					<body kind="javascript">
						<![CDATA[var Lines=[];
Lines[1]=WS.RS.Main_Charts.Chart_History_mode.Line_File_1;
Lines[2]=WS.RS.Main_Charts.Chart_History_mode.Line_File_2;
Lines[3]=WS.RS.Main_Charts.Chart_History_mode.Line_File_3;
Lines[4]=WS.RS.Main_Charts.Chart_History_mode.Line_File_4;
Lines[5]=WS.RS.Main_Charts.Chart_History_mode.Line_File_5;
Lines[6]=WS.RS.Main_Charts.Chart_History_mode.Line_File_6;
Lines[7]=WS.RS.Main_Charts.Chart_History_mode.Line_File_7;
Lines[8]=WS.RS.Main_Charts.Chart_History_mode.Line_File_8;
Lines[9]=WS.RS.Main_Charts.Chart_History_mode.Line_File_9;
Lines[10]=WS.RS.Main_Charts.Chart_History_mode.Line_File_10;
Lines[11]=WS.RS.Main_Charts.Chart_History_mode.Line_File_11;
Lines[12]=WS.RS.Main_Charts.Chart_History_mode.Line_File_12;
Lines[13]=WS.RS.Main_Charts.Chart_History_mode.Line_File_13;
Lines[14]=WS.RS.Main_Charts.Chart_History_mode.Line_File_14;
Lines[15]=WS.RS.Main_Charts.Chart_History_mode.Line_File_15;
Lines[16]=WS.RS.Main_Charts.Chart_History_mode.Line_File_16;
Lines[17]=WS.RS.Main_Charts.Chart_History_mode.Line_File_17;
Lines[18]=WS.RS.Main_Charts.Chart_History_mode.Line_File_18;
Lines[19]=WS.RS.Main_Charts.Chart_History_mode.Line_File_19;
Lines[20]=WS.RS.Main_Charts.Chart_History_mode.Line_File_20;

//DebugTool_1.Log("FileReadyCount "+Str.ToString(FileReadyCount)+","+Str.ToString(FileLoadCount));
//===========================   Режим экспорта данных   ===============================================
if (FileSaveCount>0){				// 
	if (value==FileSaveCount ){  // Если Количество записанных файлов достигло количества запросов
		var i=1;
		var S=0;
		while (i<=value) {
			S= S +Main_Panel.private.FileDataSize[i];
			i= i+1;
		}
		Main_Panel.private.MessageString_1 = "Количество сохраненных сигналов : "+(value);
		Main_Panel.private.MessageString_2 = "Суммарное количество данных : "+(S);	
		if (!Main_Panel.private.bPrint) Main_Panel.private.ShowMessage.Invoke();
	}
	if (value==-1){		// результ сохранения - ошибка
		Main_Panel.private.MessageString_1 = "Не достаточно прав для создания файла";
		Main_Panel.private.MessageString_2 = " в указанном каталоге!";
		Main_Panel.private.ShowMessage.Invoke();
	}
}
//===========================   Режим импорта данных   ===============================================
if (FileLoadCount>0){				// 
	if (value==FileLoadCount ){  // Если Количество загруженных файлов достигло количества запросов	
		log("Импорт данных выполнен "+FileLoadCount);
		var i=1;
		while (i<=value){		 		// Перебираем загруженные линии	
			Lines[i].Visible= true;	// Включаем видимость
			Lines[i].Color= ExParams.LineColor[i];	// Определяем цвет линии
			Lines[i].LineWidth= ExParams.LineWidth[i];	// Определяем толщину линии
			NowParams.Line_Width[20+i]= Lines[i].LineWidth;	// Запоминаем толщину линии в специальном массиве для изменения толщины при выделении
			NowParams.Tags[20+i]= ExParams.ItemTag[i];	
			NowParams.Description[20+i]= ExParams.Description[i];
			NowParams.Units[20+i]= ExParams.ItemEU[i];
			NowParams.Quantity[20+i]= FileDataSize[i];  // Размер считанных данных из файла
			i++;
		}
		GlobalRanges = ExParams.bGlobalRanges;	// После выполнения функции будет вызов обработчиков изменения данных свойств
		AutoRange = ExParams.bAutoRange;				// Что вызовет настройку графиков в соответсвии с заданными параметрами
		i=1;
		var S=0;
		while (i<=value) {
			var n = (ExParams.bGlobalRanges)?(0):(i);	// Номер оси для привязки в заисимости от GlobalRanges
			var id 	= WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+ExParams.Item_numAxis[n]].GetID();
			Lines[i].SetAxisById(id);
			NowParams.Item_Ymin[20+i] = ExParams.Ymin[i];
			NowParams.Item_Ymax[20+i] = ExParams.Ymax[i];
			WS.RS.Main_Charts.Chart_History_mode.Axis_Y[20+i].Color= ExParams.Ycolor[i];
			S= S +FileDataSize[i];	// Подсчет общего количества загруженных данных
			i=i+1;
		}
		NowParams.Item_Ymin[0] = Main_Panel.private.ExParams.Ymin[0];
		NowParams.Item_Ymax[0] = Main_Panel.private.ExParams.Ymax[0];
		WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Color= ExParams.Ycolor[0];
		WS.RS.Main_Charts.Chart_History_mode.Axis_X.Scale = 0;
		WS.RS.Main_Charts.Chart_History_mode.Axis_X.SetInterval(Main_Panel.private.ExParams.Xmin,Main_Panel.private.ExParams.Xmax);
		i=1;
		NowParams.LinesLoadCount = value;

		ReFresh_LegendTable.Invoke();
		MessageString_1 = "Количество загруженных сигналов : "+(value);
		MessageString_2 = "Суммарное количество данных : "+(S);	
		ShowMessage.Invoke();
		WS.RS.Main_Charts.Chart_History_mode.Update();
		WS.RS.Main_Legend.Table_1.SetSelectedRows("[]");
	}
	if (value==-1){
		MessageString_1 = "Ошибка загрузки данных из файла!";
		MessageString_2 = "";
		ShowMessage.Invoke();
	}
}]]>
					</body>
