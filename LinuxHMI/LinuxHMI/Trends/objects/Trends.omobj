	<object access-modifier="private" name="Timer_of_ReadData" display-name="Timer_of_ReadData" uuid="e337cbfa-a46c-45c3-ae15-1c192fa58008" base-type="Timer" base-type-id="72ba7cd4-549e-41f2-923c-2431b91f349a" ver="2">
		<designed target="Period" value="1000" ver="2"/>
		<designed target="Active" value="false" ver="2"/>
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="2" event="OnTimer">
			<body kind="om">
				<![CDATA[	// то что делаем всегда в любом режиме
t0:int4 = DateTime.MSecond(DateTime.Now());    // засекаем время начала работы обработчика
t1:uint8;
t2:uint8= DateTime.UtcNow();
WS.RS.Main_Charts.Chart_Operative_mode.RedLine.Value = t2;
//Main_Panel.ToolBar.RecHistory.TextEdit_1.Text = DateTime.ToString(Main_Panel.private.LeftBorderStoryInterval);
//Main_Panel.ToolBar.RecHistory.TextEdit_2.Text = DateTime.ToString(Main_Panel.private.RightBorderStoryInterval);

if (Main_Panel.private.JointMode==6){
	Main_Panel.private.JointModeHistoryRequest(false,0,0);			// надо вернуть оперативный режим
	Main_Panel.private.JointMode=2;
}
if (Main_Panel.private.JointMode==1 ) Main_Panel.private.JointMode=2;// для мерцания индикатора режима
else
	if (Main_Panel.private.JointMode==2) Main_Panel.private.JointMode=1;
	else return;

	// остальное делаем только в оперативном режиме
rb:uint8 = Variant.ToInt8(WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max, 0);		// Правая граница интервала
if (!WS.RS.Main_Charts.Chart_Operative_mode.private.bMousePressed) {	// если пользователь не менял диапазон вручную
	t3:uint8 = TypeConvert.ToUint8(Main_Panel.private.tCurInterval / 100);	// Защитный интервал для отступа от правой границы
	if (t3<10000000) t3=10000000;		// Защитный интервал минимум 1секунда
	t2 = t2 + t3;		// Сдвигаем правую границу вправо на защитный интервал
	t1 = t2- Main_Panel.private.tCurInterval;
	if (t2>rb){				// Если интервал перемещен пользователем в будущее, то интервал не меняем
		WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.SetInterval(t1,t2);	// обновляем текущий интервал
		WS.RS.Main_Charts.Chart_Operative_mode.private.omSetInterval = true;	// признак программного изменения интервала
//		DebugTool_1.Log("Сет Интервал");
	}else{
		WS.RS.Main_Charts.Chart_Operative_mode.Update();
	}
}

nStr :uint4 =0;	// вывод легенды начинается с нулевой строки
if(ActiveMode==0)									// ОПЕРАТИВНЫЙ РЕЖИМ
{																												// ЛИНИИ УРОВНЕЙ
	if (Item_Level_1.Advised) 
	{
		if (!WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_1.ApItemValueQuery_1.Active)		// флаг сбрасывается при добавлении сигнала в качестве признака необходимости 
		{																																			// однократного выполнения функций Reload и ReadRowSetAsync
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_1.ApItemValueQuery_1.Active = true;// устанавливаем флаг, активизируя запросы и ликвидируя повторный вызов функций
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_1.ApItemValueQuery_1.Reload();		// Reload и ReadRowSetAsync
 			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_1.BeginReadAsync();						
		}
	}
	if (Item_Level_2.Advised) 
	{
		if (!WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_2.ApItemValueQuery_1.Active)
		{
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_2.ApItemValueQuery_1.Active = true;
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_2.ApItemValueQuery_1.Reload();
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_2.BeginReadAsync();
		}
	}
	if (Item_Level_3.Advised)
	{
		if (!WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_3.ApItemValueQuery_1.Active)
		{
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_3.ApItemValueQuery_1.Active = true;
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_3.ApItemValueQuery_1.Reload();
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_3.BeginReadAsync();
		}
	}
	if (Item_Level_4.Advised) 
	{
		if (!WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_4.ApItemValueQuery_1.Active)
		{
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_4.ApItemValueQuery_1.Active = true;
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_4.ApItemValueQuery_1.Reload();
			WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_4.BeginReadAsync();
		}
	}
	if (Item_Level_5.Advised) 
	{
		if(!WS.RS.Main_Charts.Chart_Operative_mode.DataSource_Level_5.ApItemValueQuery_1.Active)
