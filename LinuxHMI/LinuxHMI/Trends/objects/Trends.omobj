	if (n0==8) WS.RS.Main_Charts.Chart_Operative_mode.Line_8.SetAxisById(id);
	if (n0==9) WS.RS.Main_Charts.Chart_Operative_mode.Line_9.SetAxisById(id);
	if (n0==10) WS.RS.Main_Charts.Chart_Operative_mode.Line_10.SetAxisById(id);
	id = WS.RS.Main_Charts.Chart_History_mode.Axis_Y[nAxis].GetID();  // получаем идентификатор шкалы
	WS.RS.Main_Charts.Chart_History_mode.Axis_Y[nAxis].Visible = true;
	if (n0==1) WS.RS.Main_Charts.Chart_History_mode.Line_1.SetAxisById(id); // делаем привязку к новой шкале
	if (n0==2) WS.RS.Main_Charts.Chart_History_mode.Line_2.SetAxisById(id);
