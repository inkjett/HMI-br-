						<![CDATA[if(Main_Panel.private.Reper_add==true)
{
	Main_Panel.private.CursorMode= 2;
}
i:uint4=1;

if (ActiveMode==0)		// Для оперативного режима
{
	n:int8=WS.RS.Main_Charts.Chart_Operative_mode.ControlLine.Length;	// Количество реперных линий зарезервировано
	while (i<n && WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].getActive()==false) {i=i+1;} // находим индекс активной линии
	if (i<n)	// если активная линия найдена
	{
		WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible=false; // делаем активную линию невидимой
		i=TypeConvert.ToUint4(n-1);
		while (i>0 && WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].Visible==false) {i=i-1;}	// находим индекс самой старшей видимой линии
		if (i>0)	// если видимые линии есть
		{
			WS.RS.Main_Charts.Chart_Operative_mode.ControlLine[i].setActive(true); // делаем активной самую старшую видимую линию
		}else{  // если видимых линий нет
			WS.RS.Main_Charts.Chart_Operative_mode.private.lastAddReper=0; // обнуляем счетчик реперных линий
		}
	}
	else
	{	// если активных линий не найдено
		WS.RS.Main_Charts.Chart_Operative_mode.private.lastAddReper=0; // обнуляем счетчик реперных линий
	}
	WS.RS.Main_Charts.Chart_Operative_mode.Update();
}

if (ActiveMode==1)
{
	n:int8=WS.RS.Main_Charts.Chart_History_mode.ControlLine.Length;	// Количество реперных линий зарезервировано
	while (i<n && WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].getActive()==false) {i=i+1;} // находим индекс активной линии
	if (i<n)	// если активная линия найдена
	{
		WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].Visible=false; // делаем активную линию невидимой
		i=TypeConvert.ToUint4(n-1);
		while (i>0 && WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].Visible==false) {i=i-1;}	// находим индекс самой старшей видимой линии
		if (i>0)	// если видимые линии есть
		{
			WS.RS.Main_Charts.Chart_History_mode.ControlLine[i].setActive(true); // делаем активной самую старшую видимую линию
		}else{  // если видимых линий нет
			WS.RS.Main_Charts.Chart_History_mode.private.lastAddReper=0; // обнуляем счетчик реперных линий
		}
	}
	else{	// если активных линий не найдено
		WS.RS.Main_Charts.Chart_History_mode.private.lastAddReper=0; // обнуляем счетчик реперных линий
	}
	WS.RS.Main_Charts.Chart_History_mode.Update();
}


]]>
