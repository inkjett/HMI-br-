				<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="2" event="ValueChanged">
					<body kind="javascript">
						<![CDATA[var Lines=[];
Lines[1]=WS.RS.Main_Charts.Chart_History_mode.Line_1;
Lines[2]=WS.RS.Main_Charts.Chart_History_mode.Line_2;
Lines[3]=WS.RS.Main_Charts.Chart_History_mode.Line_3;
Lines[4]=WS.RS.Main_Charts.Chart_History_mode.Line_4;
Lines[5]=WS.RS.Main_Charts.Chart_History_mode.Line_5;
Lines[6]=WS.RS.Main_Charts.Chart_History_mode.Line_6;
Lines[7]=WS.RS.Main_Charts.Chart_History_mode.Line_7;
Lines[8]=WS.RS.Main_Charts.Chart_History_mode.Line_8;
Lines[9]=WS.RS.Main_Charts.Chart_History_mode.Line_9;
Lines[10]=WS.RS.Main_Charts.Chart_History_mode.Line_10;

//DebugTool_1.Log("FileReadyCount "+Str.ToString(FileReadyCount)+","+Str.ToString(FileLoadCount));
//===========================   Режим экспорта данных   ===============================================
if (FileSaveCount>0){				// 
	if (value==FileSaveCount ){  // Если Количество записанных файлов достигло количества запросов
		var i=1;
		var S=0;
		while (i<=value) {
			S= S +Main_Panel.private.FileDataSize[i];
			i= i+1;
		}
		Main_Panel.private.MessageString_1 = "Количество сохраненных сигналов : "+(value);
		Main_Panel.private.MessageString_2 = "Суммарное количество данных : "+(S);	
		if (!Main_Panel.private.bPrint) Main_Panel.private.ShowMessage.Invoke();
	}
	if (value==-1){		// результ сохранения - ошибка
		Main_Panel.private.MessageString_1 = "Не достаточно прав для создания файла";
		Main_Panel.private.MessageString_2 = " в указанном каталоге!";
		Main_Panel.private.ShowMessage.Invoke();
	}
}
//===========================   Режим импорта данных   ===============================================
if (FileLoadCount>0){				// 
	if (value==FileLoadCount ){  // Если Количество загруженных файлов достигло количества запросов	
		log("Импорт закончен "+FileLoadCount);
		var i=1,j=1;
		while (i<=value){		 		// Перебираем загруженные линии	
			j= ExParams.numItems[i];	// Определяем номер линии в которую были загружены данные
			Lines[j].Visible= true;	// Включаем видимость
			Lines[j].Color= ExParams.LineColor[i];	// Определяем цвет линии
			Lines[j].LineWidth= ExParams.LineWidth[i];	// Определяем толщину линии
			NowParams.Line_Width[j]= Lines[j].LineWidth;	// Запоминаем толщину линии в специальном массиве для изменения толщины при выделении
			NowParams.Tags[j]= ExParams.ItemTag[i];	
			NowParams.Description[j]= ExParams.Description[i];
			NowParams.Units[j]= ExParams.ItemEU[i];
			NowParams.Quantity[j]= FileDataSize[i];  // Размер считанных данных из файла
			i++;
		}
		GlobalRanges = ExParams.bGlobalRanges;	// После выполнения функции будет вызов обработчиков изменения данных свойств
		AutoRange = ExParams.bAutoRange;				// Что вызовет настройку графиков в соответсвии с заданными параметрами
		if (ExParams.bGlobalRanges==false){		// Индивидуальный масштаб графиков включен
			i=1;
			while (i<=value) {
				var id = WS.RS.Main_Charts.Chart_History_mode.Axis_Y[ExParams.Item_numAxis[i]].GetID();
				j= ExParams.numItems[i];	// Определяем номер линии в которую были загружены данные
				Lines[j].SetAxisById(id);
				NowParams.Item_Ymin[j] = ExParams.Ymin[i];
				NowParams.Item_Ymax[j] = ExParams.Ymax[i];
//				if (j==ExParams.Item_numAxis[j]) WS.RS.Main_Charts.Chart_History_mode.Axis_Y[j].Visible = true;	// Если линия привязана к оси с таким же индексом, то делаем ось видимой
//				WS.RS.Main_Charts.Chart_History_mode.Axis_Y[j].Scale = 0; // масштаб должен поменяться в обработчике AutoRange
				WS.RS.Main_Charts.Chart_History_mode.Axis_Y[j].Color= ExParams.Ycolor[i];
				i=i+1;
			}
		}else{				// Индивидуальный масштаб графиков выключен
			LowRange = Main_Panel.private.ExParams.Ymin[0];
			HighRange = Main_Panel.private.ExParams.Ymax[0];
//			WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Visible = true;
//			WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Scale = 0; // масштаб должен поменяться в обработчике AutoRange
			WS.RS.Main_Charts.Chart_History_mode.Axis_Y[0].Color= ExParams.Ycolor[0];
		}
		WS.RS.Main_Charts.Chart_History_mode.Axis_X.Scale = 0;
		WS.RS.Main_Charts.Chart_History_mode.Axis_X.SetInterval(Main_Panel.private.ExParams.Xmin,Main_Panel.private.ExParams.Xmax);
		i=1;
		var S=0;
		while (i<=value) {	// Подсчет общего количества загруженных данных
			S= S +FileDataSize[i];
			i= i+1;
		}
		// После того как все график распределены, можно выполнить привязку к осям
		if (ExParams.bGlobalRanges==false){  // если включено индивидуальное масштабирование
			var n=0;
			while (i<=value){
				var st = ExParams.Item_numAxis[i];	// Смотрим тэг к оси котрого должен быть привязан график
				if (ExParams.Item_numAxis[i]!=ExParams.ItemTag[i]){  // Если линия привязывается не к своей оси
					var l=1;
					while (l<=value){			// Ищем индекс линии в которую загружен указанный тег
						if (ExParams.ItemTag[l]==ExParams.Item_numAxis[i]) n= ExParams.numItems[l];
						l=l+1;
					} 
				}
				if (n>0){ // Если индекс благополучно найден
					var id = WS.RS.Main_Charts.Chart_History_mode.Axis_Y[ExParams.Item_numAxis[n]].GetID();
					j= ExParams.numItems[i];	// Определяем номер линии в которую были загружены данные
					Lines[j].SetAxisById(id);  // Привязка к оси
				}
				i=i+1;
			}
		}

		WS.RS.Main_Charts.Chart_History_mode.Update();
		ReFresh_LegendTable.Invoke();
		MessageString_1 = "Количество загруженных сигналов : "+(value);
		MessageString_2 = "Суммарное количество данных : "+(S);	
		ShowMessage.Invoke();
	}
	if (value==-1){
		MessageString_1 = "Не достаточно прав для создания файла";
		MessageString_2 = " в указанном каталоге!";
		ShowMessage.Invoke();
	}
}]]>
					</body>
				</do-on>
			</object>
