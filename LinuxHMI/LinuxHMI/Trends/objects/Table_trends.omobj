	<do access-modifier="private" name="ValueRound" display-name="ValueRound" ver="2" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
		<body kind="om">
			<![CDATA[ValRound: double = Math.Round(Value,  NumDecimals);
if (NumDecimals > 0) {NumDecimals += 1;} 
else {NumDecimals = 0;}
LenValStr: int4 = Str.Length(Str.ToString(TypeConvert.ToInt4(Math.Truncate(Value)))) +  TypeConvert.ToInt4(NumDecimals);
ColForRemove: int4 = Str.Length(Str.ToString(ValRound)) - LenValStr;
ValString: string = Str.Remove(Str.ToString(ValRound), LenValStr, ColForRemove);
return ValString;

]]>
