		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="2" event="OnAdviseStateChanged">
			<body kind="om">
				<![CDATA[//pTrends.DebugTool_1.Log(Item_table.Path+" : "+Item_table.AdviseStateString);

if(Item_table.Advised==true)
{
	if (pTrends.ActiveMode==0){
		FT_t1 = Variant.ToUint8(pTrends.WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Min,0);
		FT_t2 = Variant.ToUint8(pTrends.WS.RS.Main_Charts.Chart_Operative_mode.Axis_X.Max,0);
	}else{
		FT_t1 = Variant.ToUint8(pTrends.WS.RS.Main_Charts.Chart_History_mode.Axis_X.Min,0);
		FT_t2 = Variant.ToUint8(pTrends.WS.RS.Main_Charts.Chart_History_mode.Axis_X.Max,0);	
	}
//	pTrends.DebugTool_1.Log(DateTime.ToString(FT_t1)+" - "+DateTime.ToString(FT_t2));
	Table_1.ApItemValueQuery_1.HistoricalRangeStart = FT_t1;
	Table_1.ApItemValueQuery_1.HistoricalRangeEnd = FT_t2;
	Table_1.ApItemValueQuery_1.Reload(); 
	Table_1.ReadDataAsync();
}
else
{
	Table_1.ApItemValueQuery_1.Active=false;
}]]>
			</body>
		</do-on>
