		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="2" event="MouseClick">
			<body kind="om">
				<![CDATA[Prec:uint4=1;//количество знаков после запятой
FolderPath:string=TextEdit_1.Text;
// Если папка для примера ещё не создана - создаём
if( !Files.Exists( FolderPath ) )
	Files.CreateRecursiveFolder( FolderPath );

/*
	Вот файл, в который будем писать. Используем CombinePath. Подробнее о нём
	см. в обработчике открытия формы.
*/
filePath:string = Files.CombinePath( FolderPath, TextEdit_2.Text);
here.Table_1.TableIterator.FirstSelected();
// считываем значения ячеек таблицы по одной, добавляем разделитель запятую и формируем одну большую строку.
TableText:string="Значение   Время   Качество \n";
while (!Table_1.TableIterator.End())//цикл генерации данных из таблицы
{
	TableText = TableText + ValueRound(Variant.ToDouble(Table_1.GetCellData(Table_1.TableIterator.CurrentRow,0),0),Prec);
	timestring:string = DateTime.ToString(Variant.ToUint8(Table_1.GetCellData(Table_1.TableIterator.CurrentRow,1),0)+TimeZone());	
	TableText = TableText+","+timestring;
	TableText = TableText+","+Str.ToString(Variant.ToUint8(Table_1.GetCellData(Table_1.TableIterator.CurrentRow,2),0));
	TableText = TableText+"\n";	
	DebugTool_1.Log(TableText);
	Table_1.TableIterator.Next();
}
// сохраняем в файл.
Files.WriteTextFile(filePath, TableText);

]]>
			</body>
		</do-on>
