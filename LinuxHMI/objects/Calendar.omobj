		<do access-modifier="private" name="SelectDate" display-name="SelectDate" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
			<body kind="om">
				<![CDATA[// Выбор и подсветка желаемой даты
  // Метод возвращается как только мы нашли нужную дату
  if (date == Mon.dateValue)
  {
	  Mon.Select(true);
	  return true;
  }

  if (date == Tue.dateValue)
  {
	  Tue.Select(true);
	  return true;
  }

  if (date == Wed.dateValue)
  {
	  Wed.Select(true);
	  return true;
  }

  if (date == Thu.dateValue)
  {
	  Thu.Select(true);
	  return true;
  }

  if (date == Fri.dateValue)
  {
	  Fri.Select(true);
	  return true;
  }

  if (date == Sat.dateValue)
  {
	  Sat.Select(true);
	  return true;
  }

  if (date == Sun.dateValue)
  {
	  Sun.Select(true);
	  return true;
  }

  return false;]]>
			</body>
			<param access-modifier="private" name="date" display-name="" uuid="a12ab218-3b94-42b0-9cf4-b75579902270" base-type="uint64" base-type-id="c8ff7701-da6e-4d53-888a-81fa680ff561"/>
		</do>
		<do access-modifier="private" name="SearchDefaultDate" display-name="SearchDefaultDate" ver="3" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
			<body kind="om">
				<![CDATA[// Выбор и подсветка текущей даты (defaultDate). Выделение не сбрасывается
  // Метод возвращается как только мы нашли нужную дату
  if (date == Mon.dateValue)
  {
	  Mon.SelectDefaultDay();
	  return true;
  }

  if (date == Tue.dateValue)
  {
	  Tue.SelectDefaultDay();
	  return true;
  }

  if (date == Wed.dateValue)
  {
	  Wed.SelectDefaultDay();
	  return true;
  }

  if (date == Thu.dateValue)
  {
	  Thu.SelectDefaultDay();
	  return true;
  }

  if (date == Fri.dateValue)
  {
	  Fri.SelectDefaultDay();
	  return true;
  }

  if (date == Sat.dateValue)
  {
	  Sat.SelectDefaultDay();
	  return true;
  }

  if (date == Sun.dateValue)
  {
	  Sun.SelectDefaultDay();
	  return true;
  }
  return false;]]>
			</body>
