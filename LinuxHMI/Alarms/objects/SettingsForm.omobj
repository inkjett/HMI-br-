			<![CDATA[//Формируем строку важности информационных значений как все незанятые элементы диапазона от остальных значений важности 
var str = Settings.Severity.Range1.JSON;
if (str != "")
{
	if (Settings.Severity.Range2.JSON != "") str+=","+ Settings.Severity.Range2.JSON;
	if (Settings.Severity.Range3.JSON != "") str+=","+ Settings.Severity.Range3.JSON;
}
else if (Settings.Severity.Range2.JSON != "") str = Settings.Severity.Range2.JSON;
else if (str != "") str +="," +  Settings.Severity.Range3.JSON;
		else str = Settings.Severity.Range3.JSON;
Settings.Severity.Range4.JSON = CheckSeverityRange(str,false);

//Проверяем корректность ввода полей
var range1 = CheckSeverityRange(Settings.Severity.Range1.JSON,true);
var range2 = CheckSeverityRange(Settings.Severity.Range2.JSON,true);
var range3 = CheckSeverityRange(Settings.Severity.Range3.JSON,true);
var range4 = CheckSeverityRange(Settings.Severity.Range4.JSON,true);
if (range1 == "error" || range2 == "error" || range3 == "error" || range4 == "error")
{
	bErrorFlag = true;
	//Open_Form_1.Invoke();
	return;
}

