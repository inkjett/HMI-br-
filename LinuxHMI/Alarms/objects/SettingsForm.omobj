	<do access-modifier="private" name="CheckChangeSeverity" display-name="CheckChangeSeverity" ver="3" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[//Функция проверки изменений в конфигурации важности событий
var changed = false;

//Функция преобразования JSON объекта в строку
function JSONtoString(obj)
{
	var strOut = "";
	if (obj != "") obj = JSON.parse(obj); //Проверяем наличие в JSON-строке элементов
	else return strOut;
	
	if (obj.OR != undefined)//Если в JSON-строке больше одного числа, то записываем все значения в строку
	{
		for (var i=0; i<obj.OR.length; i++)
		{
			if (i > 0) strOut+=",";
			if (obj.OR[i].severity != undefined) strOut += obj.OR[i].severity;
			if (obj.OR[i][">="] != undefined) 
			{
				strOut += obj.OR[i][">="]["severity"] + "-" + obj.OR[i+1]["<="]["severity"];
				i++;
			}
		}
	}
	if (obj.severity != undefined) strOut = obj.severity; //Если в JSON-строке только одно число
	return strOut;
}

//Заполняем поля важности фильтра
